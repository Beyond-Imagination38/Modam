{
  "rewrites": [
    {
      "source": "/a/:path*",
      "destination": "http://3.15.72.236:8080/api/:path*"
    },
    {
      "source": "/p/:path*",
      "destination": "http://3.15.72.236:8080/:path*"
    }
  ],
  "routes": [
    {
      "src": "/ws/(.*)",
      "dest": "http://3.15.72.236:8080/chat/$1",
      "headers": {
        "Connection": "upgrade",
        "Upgrade": "websocket"
      }
    }
  ]
}
